{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": " \ueb2c ",
    "foreground": "#6f6f6f"
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }} ",
          "foreground": "#ffffff",
          "background": "#404456",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " \uf07b  {{ .Path }} ",
          "foreground": "#1d3c91",
          "powerline_symbol": "\ue0b0",
          "background": "#dde4f8",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \udb80\udd93 {{ .StashCount }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#6CA35E",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }} #de7b42 {{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }} #ff4500 {{ end }}",
            "{{ if gt .Ahead 0 }} #2e86ff {{ end }}",
            "{{ if gt .Behind 0 }} #c52424 {{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}\uf42e{{ end }} ",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "type": "status",
          "style": "diamond",
          "background_templates": ["{{ if gt .Code 0 }}#f1184c{{ end }}"]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin"
          },
          "template": "<#404456,transparent>\ue0b6</>\uf520 {{ .FormattedMs }}\u202f<transparent,#404456>\ue0b2</>",
          "foreground": "#777e9d",
          "background": "#404456",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue20f {{ .Major }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b2",
          "background": "#1976d2",
          "type": "nx",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "template": " \ue753 {{ .Full }} ",
          "fetch_version": true,
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b2",
          "background": "#c4002b",
          "type": "angular",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ued0d {{.Major}} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b2",
          "background": "#6CA35E",
          "type": "node",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "type": "pnpm",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "foreground": "#000000",
          "background": "#F9AD00",
          "template": " \ue865 {{ .Major }} ",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "\udb80\udcf6 Jan 2  \uf017 15:04"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#777e9d",
          "background": "#404456",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
